# ===== INDEXER CONFIGURATION =====
# Development indexer
VITE_INDEXER_URL_DEV=http://localhost:42069/api/proxy/graphql
VITE_INDEXER_TOKEN_DEV=dev-secret-change-in-production

# Production indexer  
VITE_INDEXER_URL_PROD=https://your-indexer.com/api/proxy/graphql
VITE_INDEXER_TOKEN_PROD=prod-secret-change-in-production

# ===== ACCOUNT ABSTRACTION =====
# Pimlico Bundler API Key for Account Abstraction
# Get your API key from https://dashboard.pimlico.io/
VITE_PIMLICO_API_KEY=your_pimlico_api_key_here

# ===== WALLET INTEGRATION =====
# WalletConnect Project ID for Rainbow wallet integration (browser only)
# Get this from https://cloud.walletconnect.com/
VITE_WC_PROJECT_ID=your_project_id_here

# ===== OPTIONAL =====
# Custom RPC endpoints
VITE_ALCHEMY_API_KEY=